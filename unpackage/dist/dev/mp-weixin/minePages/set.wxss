/* 用户头像 start */
.logo-image.data-v-56d8c034 {
  width: 210rpx;
  height: 210rpx;
  position: relative;
}
.about-shadow.data-v-56d8c034 {
  border-radius: 15rpx;
  box-shadow: 0rpx 50rpx 50rpx 50rpx rgba(0, 0, 0, 0.07);
}
.logo-pic.data-v-56d8c034 {
  background-size: cover;
  background-repeat: no-repeat;
  background-position: top;
  border: 8rpx solid rgba(255, 255, 255, 0.05);
  box-shadow: 0rpx 0rpx 80rpx 0rpx rgba(0, 0, 0, 0.15);
  border-radius: 50%;
  overflow: hidden;
}
.bg-flex-shadow.data-v-56d8c034 {
  background-color: #00C3FF;
  z-index: 9999;
}
/* 胶囊*/
.tn-custom-nav-bar__back.data-v-56d8c034 {
  width: 100%;
  height: 100%;
  position: relative;
  display: flex;
  justify-content: space-evenly;
  align-items: center;
  box-sizing: border-box;
  background-color: rgba(0, 0, 0, 0.15);
  border-radius: 1000rpx;
  border: 1rpx solid rgba(255, 255, 255, 0.5);
  color: #FFFFFF;
  font-size: 18px;
}
.tn-custom-nav-bar__back .icon.data-v-56d8c034 {
  display: block;
  flex: 1;
  margin: auto;
  text-align: center;
}
.tn-custom-nav-bar__back.data-v-56d8c034:before {
  content: " ";
  width: 1rpx;
  height: 110%;
  position: absolute;
  top: 22.5%;
  left: 0;
  right: 0;
  margin: auto;
  -webkit-transform: scale(0.5);
          transform: scale(0.5);
  -webkit-transform-origin: 0 0;
          transform-origin: 0 0;
  pointer-events: none;
  box-sizing: border-box;
  opacity: 0.7;
  background-color: #FFFFFF;
}
/* 底部悬浮按钮 start*/
.tn-tabbar-height.data-v-56d8c034 {
  min-height: 100rpx;
  height: calc(120rpx + env(safe-area-inset-bottom) / 2);
}
.tn-footerfixed.data-v-56d8c034 {
  position: fixed;
  width: 100%;
  bottom: calc(30rpx + env(safe-area-inset-bottom));
  z-index: 1024;
  box-shadow: 0 1rpx 6rpx rgba(0, 0, 0, 0);
}
/* 底部悬浮按钮 end*/
